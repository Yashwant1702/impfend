# Campus Club Suite Environment Variables

# Application Configuration
REACT_APP_NAME=Campus Club Suite
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=30000

# Authentication
REACT_APP_AUTH_STORAGE_KEY=campus_club_auth
REACT_APP_JWT_SECRET=your-jwt-secret-key-here
REACT_APP_SESSION_TIMEOUT=86400000

# Features Flags
REACT_APP_ENABLE_GAMIFICATION=true
REACT_APP_ENABLE_MESSAGING=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_FILE_UPLOADS=true

# External Services
REACT_APP_UPLOAD_SERVICE_URL=http://localhost:8000/api/uploads
REACT_APP_WEBSOCKET_URL=ws://localhost:8000/ws
REACT_APP_CDN_URL=http://localhost:8000/media

# Analytics & Monitoring
REACT_APP_ANALYTICS_ID=
REACT_APP_SENTRY_DSN=
REACT_APP_ENABLE_ANALYTICS=false

# Social Media Integration
REACT_APP_FACEBOOK_APP_ID=
REACT_APP_GOOGLE_CLIENT_ID=
REACT_APP_LINKEDIN_CLIENT_ID=

# Email Configuration
REACT_APP_SUPPORT_EMAIL=support@campusclubsuite.com
REACT_APP_CONTACT_EMAIL=contact@campusclubsuite.com

# Campus Information
REACT_APP_CAMPUS_NAME=Your University
REACT_APP_CAMPUS_TIMEZONE=America/New_York
REACT_APP_CAMPUS_LOCALE=en-US

# Debug & Development
REACT_APP_DEBUG_MODE=false
REACT_APP_SHOW_DEV_TOOLS=false
